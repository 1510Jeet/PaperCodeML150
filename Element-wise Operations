import numpy as np
from typing import Dict

def elementwise_ops(a: np.ndarray, b: np.ndarray) -> Dict[str, np.ndarray]:
    """
    Computes element-wise add, mul, and safe div.
    
    Args:
        a: First tensor
        b: Second tensor (same shape)
        
    Returns:
        Dictionary with keys "add", "mul", "div"
    """
    epsilon = 1e-8
    ans={}
    ans["add"]=a+b
    ans["mul"]=a*b
    ans["div"]=a/(b+epsilon)
    return ans
